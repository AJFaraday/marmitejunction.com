<h1>Listing junction_embeds</h1>

<table>
  <tr>
    <th>Added on</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @junction_embeds.each do |junction_embed| %>
  <tr>
    <td colspan=4><%= raw junction_embed.code %></td>
  </tr>
  <tr>
    <td><%= junction_embed.created_at.strftime("%d %b %Y") %></td>
    <td><%= link_to 'Show', junction_embed %></td>
    <td><%= link_to 'Edit', edit_junction_embed_path(junction_embed) %></td>
    <td><%= link_to 'Destroy', junction_embed, :method => :delete, :confirm => 'Are you sure?' %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Junction embed', new_junction_embed_path %>
